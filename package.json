    "name": "tryrenovate",
    "version": "0.0.1",
    "description": "Try renovate update dependencies demo",
    "main": "index.js",
    "repository": "git@github.com:sandinosaso/tryrenovate.git",
    "author": "Someone",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": "14.15.0"
    },
    "engineStrict": true,
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "packages/groceries-menu-microfrontend/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix --quiet"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint -c .eslintrc-next.js --fix --quiet",
            "prettier --write",
            "git add"
        ],
        "*.{js,scss,md,json,yml}": [
            "prettier --write",
            "git add"
        ]
    },
    "scripts": {
        "bootstrap-lerna": "lerna bootstrap",
        "build": "yarn build:client && yarn build:libs && yarn build:sw",
        "build:all": "concurrently --kill-others-on-fail \"yarn build:client:legacy\" \"yarn build:client\" \"yarn build:libs\" \"yarn bundle:libs:location\" \"yarn build:sw\" \"yarn build:server\"",
        "build:client": "TS_NODE_PROJECT=tsconfig-cjs.json webpack --config webpack-config-client/config.ts",
        "build:client:legacy": "TS_NODE_PROJECT=tsconfig-cjs.json webpack --config webpack-config-client/config.es5.ts",
        "build:config": "lerna run build --scope @deliveryhero/pd-mfe-config --stream",
        "build:gears": "lerna run build --scope @deliveryhero/pd-mfe-gears --stream",
        "build:package": "yarn zx scripts/build-package.mjs",
        "build:services": "lerna run build --scope @deliveryhero/pd-mfe-services --stream",
        "build:providers": "lerna run build --scope @deliveryhero/pd-mfe-providers --stream",
        "build:libs": "TS_NODE_PROJECT=tsconfig-cjs.json webpack --config webpack-config-client/config.libs.ts",
        "build:library-payments": "lerna run build --scope @deliveryhero/pd-mfe-library-payments --stream",
        "build:server": "TS_NODE_PROJECT=tsconfig-cjs.json  webpack --config webpack-config-server/config.ts",
        "build:sw": "TS_NODE_PROJECT=tsconfig-cjs.json  webpack --config webpack-config-client/config.sw.ts",
        "bundle:package": "yarn zx scripts/bundle-package.mjs",
        "bundle:libs:location": "lerna run bundle-webpack --scope @deliveryhero/pd-mfe-library-location --stream",
        "debug:server": "LOCAL_DEBUGGER=true yarn start",
        "dev": "export WITH_PROXY=true; yarn tsnode -r packages/env/inject-env webpack-config-server",
        "fetchTranslations": "ts-node --files --project tsconfig-cjs.json -r tsconfig-paths/register -r dotenv/config core/gears/translation-fallback/fetch-translations.ts init-fetch-service",
        "lerna": "lerna",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "package:gears": "rollup -c ./core/gears/rollup.config.js",
        "prettier": "prettier --ignore-path .gitignore ./**/*.{ts,tsx,js,css,scss,md,json,yml,d.ts}",
        "dev:reconfigure": "yarn tsnode -r packages/env/inject-env packages/env/validate-env/validate-cli --reconfigure",
        "start": "TS_NODE_PROJECT=tsconfig-cjs.json webpack -r packages/env/inject-env --config webpack-config-server/config.ts",
        "start:with-dev-proxy": "echo This command has been renamed to \\\"yarn dev\\\" to avoid confusion and will be removed in the future",
        "start:graphql": "yarn updateGraphql && yarn start",
        "start:prod": "NODE_ENV=production; node --max_old_space_size=15192 -r ./build-server/inject-env.js build-server/$APPLICATION-server.js",
        "stats": "webpack-bundle-analyzer build/stats.json -m static",
        "stats:build": "TS_NODE_PROJECT=tsconfig-cjs.json webpack -r packages/env/inject-env --config webpack-config-client/config.ts --profile --json > build/stats.json",
        "test": "jest --config jest/jest.config.js",
        "test:config": "lerna run test --scope @deliveryhero/pd-mfe-config --stream",
        "test:changes": "yarn test --coverage --changedSince=origin/master",
        "test:ci": "jest --config jest/jest-ci.config.js",
        "test:ci:changes": "yarn test:ci --coverage --changedSince=origin/master --json --outputFile=jest.results.json",
        "test:gears": "lerna run test --scope @deliveryhero/pd-mfe-gears --stream",
        "test:hooks": "lerna run test --scope @deliveryhero/pd-mfe-hooks --stream",
        "test:library-payments": "lerna run test --scope @deliveryhero/pd-mfe-library-payments --stream",
        "test:services": "lerna run test --scope @deliveryhero/pd-mfe-services --stream",
        "test:providers": "lerna run test --scope @deliveryhero/pd-mfe-providers --stream",
        "test:libs:location": "lerna run test --scope @deliveryhero/pd-mfe-library-location --stream",
        "test:types": "yarn tsc",
        "tsnode": "ts-node --max_old_space_size=15192 --project tsconfig-cjs.json -r tsconfig-paths/register",
        "updateGraphql": "./update-graphql.sh",
        "webpack": "node --max-old-space-size=8192 node_modules/.bin/webpack",
        "watch": "yarn start --watch",
        "zx": "zx"
    },
